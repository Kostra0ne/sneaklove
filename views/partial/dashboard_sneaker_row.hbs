<td class="table-division">
  <div><img src="{{this.image}}" alt="img" class="table-image" /></div>
</td>
<td class="table-division">
  <div>{{this.name}}</div>
</td>
<td class="table-division">
  <div>{{this.ref}}</div>
</td>
<td class="table-division">
  <div>${{this.price}}</div>
</td>
<td class="table-division">
  <a href="/dashboard_sneaker/product-edit/{{this._id}}" data-id-sneaker="{{this._id}}">
