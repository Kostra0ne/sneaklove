<td class="table-division">
  <div><img src="{{this.image}}" alt="img" class="table-image" /></div>
</td>
<td class="table-division">
  <div>{{this.name}}</div>
</td>
<td class="table-division">
  <div>{{this.ref}}</div>
</td>
<td class="table-division">
  <div>${{this.price}}</div>
</td>
<td class="table-division">
  <a href="/dashboard/update/{{this._id}}" class="fa fa-edit table-icon" data-id-sneaker="{{this._id}}">
</td></a>
<td class="table-division">
  <a href="/dashboard/delete/{{this._id}}"><span class="fa fa-trash table-icon" data-id-sneaker="{{this._id}}"
      aria-hidden="true"></span>
</td></a>
</td>